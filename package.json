    "jest-mock-promise": "^2.0.2",
    "jest-resolve": "29.0.3",
    "jest-watch-typeahead": "^2.2.0",
    "lint-staged": "^10.5.3",
    "lodash.throttle": "^4.1.1",
    "mini-css-extract-plugin": "^2.4.1",
    "nodemon": "^2.0.21",
    "prettier": "^3.0.0",
    "prop-types": "^15.5.10",
